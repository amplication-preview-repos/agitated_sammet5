datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AiModel {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model Trade {
  createdAt DateTime            @default(now())
  id        String              @id @default(cuid())
  price     Float?
  quantity  Int?
  status    EnumTradeStatus?
  tradeDate DateTime?
  tradeType EnumTradeTradeType?
  updatedAt DateTime            @updatedAt
  user      User?               @relation(fields: [userId], references: [id])
  userId    String?
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  trades    Trade[]
  updatedAt DateTime @updatedAt
  username  String   @unique
}

enum EnumTradeStatus {
  Option1
}

enum EnumTradeTradeType {
  Option1
}
